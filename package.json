{
    "name": "execution-monitor",
    "displayName": "Execution Monitor",
    "description": "Visualize monitored function executions in Python files",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.78.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:python"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.selectExecution",
                "title": "Select Execution"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "package": "vsce package"
    },
    "devDependencies": {
        "@types/vscode": "^1.78.0",
        "@types/node": "16.x",
        "typescript": "^4.9.4",
        "vsce": "^2.15.0"
    },
    "dependencies": {
        "axios": "^1.4.0"
    }
} 